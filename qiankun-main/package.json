{
  "name": "qiankun-main",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "install": "npm-run-all --serial install:*",
    "install:qiankun-main": " npm i",
    "install:qiankun-vue-child": "cd ../ && cd qiankun-vue-child && npm i",
    "install:qiankun-vue-child22": "cd ../ && cd qiankun-vue-child22 && npm i",
    "install:qiankun-vue-child333": "cd ../ && cd qiankun-vue-child333 && npm i",
    "start": "npm-run-all --parallel start:*",
    "start:qiankun-main": "yarn serve",
    "start:qiankun-vue-child": "cd ../ && cd qiankun-vue-child && yarn serve",
    "start:qiankun-vue-child22": "cd ../ && cd qiankun-vue-child22 && yarn serve",
    "start:qiankun-vue-child333": "cd ../ && cd qiankun-vue-child333 && yarn serve"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "npm-run-all": "^4.1.5",
    "qiankun": "^2.3.5",
    "vue": "^2.6.11",
    "vue-router": "^3.4.9",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
